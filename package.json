{
  "name": "ngdg",
  "version": "1.0.0",
  "description": "A new dildo generator",
  "browser": "src/cjs/index.js",
  "main": "src/cjs/index.js",
  "module": "src/esm/index.js",
  "types": "src/cjs/index.d.ts",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/earcut": "^2.1.1",
    "@types/offscreencanvas": "^2019.6.4",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "earcut": "^2.2.3",
    "eslint": "^7.32.0",
    "path": "^0.12.7",
    "plotboilerplate": "^1.12.6",
    "rollup": "^2.57.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "source-map-loader": "^3.0.0",
    "three": "^0.124.0",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "FileSaver": "^0.10.0",
    "alloyfinger": "^0.1.16",
    "dat.gui": "^0.7.7",
    "touchy": "^0.1.6",
    "ts-loader": "^7.0.5"
  },
  "scripts": {
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src/ts --extensions '.ts' --out-dir 'dist/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src/ts --extensions '.ts' --out-dir 'dist/cjs' --source-maps",
    "build": "npm run build-module && npm run build-browser",
    "build-browser": "npm run compile-typescript-browser && npm run webpack-browser && cp dist/plotboilerplate.browser.min.js dist/plotboilerplate.min.js",
    "build-module": "npm run compile-typescript-module && npm run rollup-module",
    "compile-typescript-browser": "tsc -p tsconfig.browser.json && npm run copy-to-public",
    "compile-typescript-module": "tsc -p tsconfig.module.json",
    "copy-to-public": "cp -R src public/lib/ngdg && cp node_modules/three/build/three.min.js public/lib/three/build/three.min.js && cp -R node_modules/plotboilerplate/src/ public/lib/plotboilerplate/src",
    "mkdocs": "cd bin && ./run-mk-typedocs.sh",
    "server": "npx http-server ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack-browser": "npx webpack --mode production --config ./webpack5.browser.config.js",
    "webpack-dev": "npx webpack --mode development --config ./webpack5-dev.config.js",
    "rollup-module": "npx rollup -c",
    "tslint": "npx eslint ./src/ts/"
  },
  "author": "",
  "license": "ISC"
}
